<div class="register-box">

  <div class="register-logo">
        <b>Cadastro de viatura</b>
  </div>

  <div class="register-box-body">

    <p class="login-box-msg">Informe os dados abaixo para cadastrar uma viatura.</p>

	<%= form_for(@vehicle) do |f| %>
		<%= render 'create_fields', f: f %>
		<%= f.submit "Inserir viatura", class: "btn btn-large btn-primary",  data: { confirm: 'Deseja cadastrar a viatura com os dados informados?' } %>
	<% end %>

  </div><!-- /.form-box -->
</div>